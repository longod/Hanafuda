# https://aka.ms/editorconfigdocs
# http://editorconfig.org/

# root = true

# All files
[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# markdown
[*.{md,markdown}]
trim_trailing_whitespace = false
